اشمل "مـتم/طـرفية"؛

استخدم مـتم؛


دالة ينتمي_للأبجدية(م: مـحرف): ثـنائي {
	ارجع ( م <= 'Z' && م >= 'A') || ( م <= 'z' && م >= 'a')؛
}


دالة رئيسي() {
	عرف م1: مـحرف = 'f'؛
	إذا ينتمي_للأبجدية(م1) {
		طـرفية.اطبع("صح\ج")؛
	}
	وإلا {
		طـرفية.اطبع("خطأ\ج")؛
	}
	
	عرف م2: مـحرف = '&'؛
	إذا ينتمي_للأبجدية(م2) {
		طـرفية.اطبع("صح\ج")؛
	}
	وإلا {
		طـرفية.اطبع("خطأ\ج")؛
	}
}

رئيسي()؛
