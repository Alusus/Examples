اشمل "مـتم/طـرفية"؛
اشمل "مـتم/نـص"؛

استخدم مـتم؛


دالة المحرف_متواجد(س: سند[نـص]، م: مـحرف): ثـنائي {
	عرف دليل: صـحيح؛
	لكل دليل = 0 ، دليل < س.هات_الطول() ، دليل++ {
		إذا س(دليل) == م {
			ارجع صح؛
		}
	}
	ارجع خطأ؛
}


دالة رئيسي() {
	عرف س1: نـص = "abcde1324"؛
	إذا المحرف_متواجد(س1، 'c') {
		طـرفية.اطبع("صح\ج")؛
	}
	وإلا {
		طـرفية.اطبع("خطأ\ج")؛
	}

	إذا المحرف_متواجد(س1، '0') {
		طـرفية.اطبع("صح\ج")؛
	}
	وإلا {
		طـرفية.اطبع("خطأ\ج")؛
	}
}

رئيسي()؛
