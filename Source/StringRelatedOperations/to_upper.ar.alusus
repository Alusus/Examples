اشمل "مـتم/نـص"؛

استخدم مـتم؛


دالة إلى_حالة_كبيرة(س: سند[نـص]): نـص {
	عرف نتيجة: نـص = ""؛

	عرف دليل: صـحيح؛
	عرف م: مـحرف؛
	لكل دليل = 0 ، دليل < س.هات_الطول() ، دليل++ {
		إذا س(دليل) >= 'a' && س(دليل) <= 'z' {
			م = س(دليل) - 'a' + 'A'؛
		}
		وإلا {
			م = س(دليل)؛
		}
		نتيجة += م؛
	}
	ارجع نتيجة؛
}
