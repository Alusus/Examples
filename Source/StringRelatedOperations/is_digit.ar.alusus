اشمل "مـتم/طـرفية"؛

استخدم مـتم؛


دالة ينتمي_للأرقام(م: مـحرف): ثـنائي {
	ارجع ( م <= '9' && م >= '0')؛
}


دالة رئيسي() {
	عرف م1: مـحرف = 'f'؛
	إذا ينتمي_للأرقام(م1) {
		طـرفية.اطبع("صح\ج")؛
	}
	وإلا {
		طـرفية.اطبع("خطأ\ج")؛
	}
	
	عرف م2: مـحرف = '5'؛
	إذا ينتمي_للأرقام(م2) {
		طـرفية.اطبع("صح\ج")؛
	}
	وإلا {
		طـرفية.اطبع("خطأ\ج")؛
	}
}

رئيسي()؛
