import "Srl/Console";
import "Srl/String";

use Srl;



func main() {
    def s: String("Hello, world!");

	def size: Int = s.getLength();
    def result: String = "";

    def i: Int;
    for i = size - 1 , i >= 0 , i-- {
        result += s(i);
    }

    Console.print("%s\n", result.buf);
}

main();
