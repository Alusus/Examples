اشمل "مـتم/طـرفية"؛
اشمل "مـتم/نـص"؛

استخدم مـتم؛


دالة يبدأ_ب(س: سند[نـص]، سابق: سند[نـص]): ثـنائي {
	إذا س.هات_الطول() < سابق.هات_الطول() {
		ارجع خطأ؛
	}

	عرف دليل: صـحيح؛
	لكل دليل = 0 ، دليل < سابق.هات_الطول() ، دليل++ {
		إذا سابق(دليل) != س(دليل) {
			ارجع خطأ؛
		}
	}
	ارجع صح؛
}


دالة رئيسي() {
	عرف س: نـص = "abccde1324"؛
	عرف سابق: نـص = "abc"؛
	طـرفية.اطبع("%d\ج"، يبدأ_ب(س، سابق))؛
	سابق = "abcd"؛
	طـرفية.اطبع("%d\ج"، يبدأ_ب(س، سابق))؛
}

رئيسي()؛
