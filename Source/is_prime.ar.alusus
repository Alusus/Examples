اشمل "مـتم/طـرفية"؛

استخدم مـتم؛


دالة هل_أولي (عدد: صـحيح): ثـنائي {
	إذا عدد <= 1 {
		ارجع خطأ؛
	}
	
	عرف دليل: صـحيح؛
	لكل دليل = 2 ، دليل * دليل <= عدد ، دليل++ {
		إذا عدد % دليل == 0 {
			ارجع خطأ؛
		}
	}
	
	ارجع صح؛
}

دالة رئيسي() {
	عرف عدد: صـحيح = 59؛
	
	إذا هل_أولي(عدد) {
		طـرفية.اطبع("%d is a prime number"، عدد)؛
	}
	وإلا {
		طـرفية.اطبع("%d is not a prime number"، عدد)؛
	}
}

رئيسي()؛
