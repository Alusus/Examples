import "Srl/Console";
import "Srl/Array";

use Srl;

func f(element: Int): Int {
    return element * 2;
}

func main() {
    def a: Array[Int]({1, 2, 3, 4, 5});

    def i: Int;
    
    for i = 0 , i < a.getLength() , i++ {
        a(i) = f(a(i));
    }

    for i = 0 , i < a.getLength() , i++ {
        Console.print("%d ", a(i));
    }
}

main();
