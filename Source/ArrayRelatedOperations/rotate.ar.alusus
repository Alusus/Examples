اشمل "مـتم/مـصفوفة"؛
اشمل "مـتم/طـرفية"؛

استخدم مـتم؛


دالة دور(م: سند[مـصفوفة[صـحيح]]) {
	عرف حجم: صـحيح = م.هات_الطول()؛
	
	عرف أول_عنصر: صـحيح = م(0)؛
	
	عرف دليل: صـحيح؛
	لكل دليل = 0 ، دليل < حجم - 1 ، دليل++ {
		م(دليل) = م(دليل + 1)؛
	}
	
	م(حجم - 1) = أول_عنصر؛
}

دالة رئيسي() {
	عرف م: مـصفوفة[صـحيح]({1, 2, 3, 4, 5, 6})؛
	
	دور(م)؛
	
	عرف دليل: صـحيح؛
	لكل دليل = 0 ، دليل < م.هات_الطول() ، دليل++ {
		طـرفية.اطبع("%d "، م(دليل))؛
	}
}

رئيسي()؛
